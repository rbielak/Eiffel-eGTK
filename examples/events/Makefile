ROOT=$(EGTK)/examples/events
EIFFEL_ROOT_CLASS= events_tester
EIFFEL_CREATION_ROUTINE= make

all: 
	$(EGTK)/bin/egtkbuild events_tester

clean:
	-rm -f *.o *.h *.c *~
	-rm -f $(EIFFEL_ROOT_CLASS)
	-rm -f *.make

