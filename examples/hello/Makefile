ROOT=$(EGTK)/examples/hello
EIFFEL_ROOT_CLASS= hello2
EIFFEL_CREATION_ROUTINE= make

all: 
	$(EGTK)/bin/egtkbuild hello2

clean:
	-rm -f *.o *.h *.c *~
	-rm -f $(EIFFEL_ROOT_CLASS)
	-rm -f *.make

