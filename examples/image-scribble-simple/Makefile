ROOT=$(EGTK)/examples/image_scribble_app
EIFFEL_ROOT_CLASS=image_scribble_app
EIFFEL_CREATION_ROUTINE= make

all:
	$(EGTK)/bin/egtkbuild $(EIFFEL_ROOT_CLASS)

clean:
	-rm -f *.o *.h *.c *~
	-rm -f $(EIFFEL_ROOT_CLASS)
	-rm -f *.make
